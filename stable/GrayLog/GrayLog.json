eep 15\n\nsudo curl -XGET 'http:\/\/localhost:9200\/_cluster\/health?pretty=true'\n\n\n#Install graylog\nsudo rpm -Uvh https:\/\/packages.graylog2.org\/repo\/packages\/graylog-2.5-repository_latest.rpm\nsudo yum -y install graylog-server\n\n#Configure graylog\n\nsudo sed -i '\/password_secret\/s\/^\/#\/' \/etc\/graylog\/server\/server.conf\nsudo sed -i '\/root_password_sha2\/s\/^\/#\/' \/etc\/graylog\/server\/server.conf\nsudo sed -i '\/elasticsearch_shards\/s\/^\/#\/' \/etc\/graylog\/server\/server.conf\n\npwd_secret=$(sudo pwgen -N 1 -s 96)\nroot_pwd=$(echo -n @@{ADMIN_PASSWORD}@@ | sha256sum | awk '{print $1}')\n\n\necho \"password_secret=$pwd_secret\nroot_password_sha2=$root_pwd\nroot_email=@@{ADMIN_EMAIL}@@\nroot_timezone=UTC\n#elasticsearch_discovery_zen_ping_unicast_hosts = @@{address}@@:9300\nelasticsearch_shards=1\nscript.inline: false\nscript.indexed: false\nscript.file: false\" | sudo tee -a \/etc\/graylog\/server\/server.conf\n\n\n\nsudo sed -i '\/web_listen_uri\/s\/^#\/\/' \/etc\/graylog\/server\/server.conf\n\nsudo sed -i '\/rest_listen_uri\/s\/127.0.0.1\/0.0.0.0\/' \/etc\/graylog\/server\/server.conf\nsudo sed -i '\/web_listen_uri\/s\/127.0.0.1\/0.0.0.0\/' \/etc\/graylog\/server\/server.conf\n\necho 'web_endpoint_uri = http:\/\/@@{address}@@:9000\/api' | sudo tee -a \/etc\/graylog\/server\/server.conf","script_type":"sh","type":"","command_line_args":"","login_credential_local_reference":{"kind":"app_credential","name":"centos","uuid":"2e253222-f95c-4549-8190-b14394901371"}},"timeout_secs":"0","message_list":[],"variable_list":[],"name":"PackageInstallTask"}],"description":"","message_list":[],"uuid":"bc665f3a-6016-4142-b74a-5f523b0f9583","state":"ACTIVE","variable_list":[],"main_task_local_reference":{"kind":"app_task","name":"d941afcc_dag","uuid":"9c8d3aad-b259-41d4-a9c4-565e7313422f"},"name":"7d894f86_runbook_cloned_1"},"type":"","uninstall_runbook":{"task_definition_list":[{"target_any_local_reference":{"kind":"app_package","name":"InstallAzureGra