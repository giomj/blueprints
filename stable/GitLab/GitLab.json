                                        "script": "#!/bin/bash\nset -ex\n\nsudo mkdir -p /opt/gitlab\nsudo yum install -y lvm2 --quiet\nsudo pvcreate /dev/sd{b,c,d}\nsudo vgcreate gitlab /dev/sd{b,c,d}\nsleep 3\nsudo lvcreate -l 100%VG -n gitlab_lvm gitlab\nsudo mkfs.xfs /dev/gitlab/gitlab_lvm\n\necho -e \"/dev/gitlab/gitlab_lvm \\t /opt/gitlab \\t xfs \\t defaults \\t 0 0\" | sudo tee -a /etc/fstab\nsudo mount -a"
                                    },
